use imdb_ijs;

drop table if exists movies_recommendations;

create table movies_recommendations (
  base_movie_id int,
  recommended_movie_id int,
  recommendation int,
  suggested_by varchar(255),
  justification varchar(255),
  comment varchar(255),
  CONSTRAINT CHK_recommendation CHECK (recommendation >= 1 AND recommendation <= 10)
);

